    "@tanstack/react-query": "^5.51.21",
    "@tanstack/react-router": "^1.48.1",
    "@tanstack/router-devtools": "^1.48.1",
    "@tauri-apps/api": "2.0.0-rc.0",
    "@tauri-apps/plugin-deep-link": "2.0.0-rc.0",
    "@tauri-apps/plugin-shell": "2.0.0-rc.0",
